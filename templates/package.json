{
  "private": true,
  "engines": {
    "node": ">=6",
    "npm": ">=3.8"
  },
  "dependencies": {
    "electron": "^1.3.3",
    "electron-debug": "^1.0.1",
    "history": "^2.1.2",
    "react": "^15.3.0",
    "react-app": "^1.1.2",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2"
  },
  "devDependencies": {
    "electron-react-app-scripts": "^0.0.2"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-1"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
      "browser": true
    },
    "rules": {
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js"
          ]
        }
      ]
    }
  },
  "scripts": {
    "postinstall": "electron-react-app clean-cache",
    "lint": "eslint actions components core routes test utils main.js",
    "build": "electron-react-app build",
    "start": "electron-react-app start"
  }
}
